{
    "name": "crypt-keeper-extension",
    "version": "1.0.0",
    "description": "Crypt-Keeper Extension, zero knowledge identity management and proof generation tool",
    "main": "index.js",
    "scripts": {
        "build": "NODE_ENV=production webpack --config webpack.prod.js && npm run clean-background-prod",
        "dev": "NODE_ENV=development webpack --config webpack.dev.js && npm run clean-background-dev",
        "clean-background-dev": "source scripts/clean.sh",
        "clean-background-prod": "source scripts/clean-prod.sh",
        "serve": "./scripts/serve.sh",
        "merkle": "ts-node scripts/mock-merkle-proof.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "prettier": "prettier -c .",
        "prettier:fix": "prettier -w .",
        "commit": "cz",
        "precommit": "lint-staged"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com"
    },
    "keywords": [
        "react",
        "typescript",
        "chrome",
        "extension",
        "boilerplate"
    ],
    "author": "Privacy and Scaling explorations team",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/privacy-scaling-explorations/crypt-keeper-extension"
    },
    "homepage": "https://github.com/privacy-scaling-explorations/crypt-keeper-extension",
    "devDependencies": {
        "@types/eventemitter2": "^4.1.0",
        "@types/express": "^4.17.16",
        "@types/jest": "^29.2.2",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.8",
        "@types/react-router-dom": "^5.3.3",
        "@types/redux-logger": "^3.0.9",
        "commitizen": "^4.2.5",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.7.2",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.27.0",
        "express": "^4.18.2",
        "file-loader": "^6.2.0",
        "http-server": "^14.1.1",
        "image-webpack-loader": "^8.1.0",
        "jest": "^29.3.1",
        "path-browserify": "^1.0.1",
        "prettier": "^2.7.1",
        "readline": "^1.3.0",
        "sass": "^1.56.1",
        "sass-loader": "^13.2.0",
        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.4.1",
        "ts-node": "^10.9.1",
        "webpack": "^5.75.0",
        "webpack-cli": "^4.10.0",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@dimensiondev/metamask-extension-provider": "https://github.com/DimensionDev/extension-provider/tarball/master",
        "@interep/identity": "^0.3.0",
        "@semaphore-protocol/group": "^2.6.1",
        "@semaphore-protocol/identity": "^2.6.1",
        "@semaphore-protocol/proof": "^2.6.1",
        "@types/classnames": "^2.3.0",
        "@zk-kit/incremental-merkle-tree": "^1.0.0",
        "@zk-kit/protocols": "^1.11.1",
        "axios": "^1.1.3",
        "bigint-conversion": "^2.3.0",
        "browserify": "^17.0.0",
        "classnames": "^2.3.2",
        "copy-to-clipboard": "^3.3.2",
        "crypto-js": "^4.1.1",
        "ethers": "^5.7.2",
        "eventemitter2": "^6.4.9",
        "fast-deep-equal": "^3.1.3",
        "link-preview-js": "^3.0.4",
        "loglevel": "^1.8.1",
