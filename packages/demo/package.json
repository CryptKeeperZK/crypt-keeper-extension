    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@cryptkeeperzk/eslint-config-react": "workspace:^",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18.2.13",
    "assert": "^2.1.0",
    "eslint": "^8.51.0",
    "lint-staged": "^15.0.1",
